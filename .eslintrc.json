{
  "extends": [
    "next/core-web-vitals",
    "prettier",
    "eslint:recommended",
    "plugin:tailwindcss/recommended"
  ],
  "plugins": [
    "tailwindcss"
  ],
  "rules": {
    "react/no-unescaped-entities": "off",
    "no-debugger": "error",
    "no-console": "error",
    "no-alert": "error"
  }
}
